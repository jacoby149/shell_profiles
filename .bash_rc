#display messages
echo "commands - see a list of commands"
echo "links - see a list of links"
function commands(){echo "see dc dq serve profiles vc refresh"}
function links(){echo "home box g web10"}

# commands
function see(){open .}
function dc(){docker-compose up --build}
function dq(){docker-compose down}
function serve(){open -a "Google Chrome" "http://localhost:8000" && python -m http.server}
function profiles(){code ~/Documents/GitHub/shell_profiles}
function vc(){code .}
function refresh(){source ~/.zshrc}

#links
function box(){cd ~/Dropbox}
function g(){cd ~/Documents/GitHub}
function web10(){cd ~/Documents/GitHub/web10}
function home(){cd ~}