#display messages
echo "commands - see a list of commands";
echo "links - see a list of links";
function commands(){echo "see dc dq serve profiles vc refresh ranger"}
function links(){echo "home box g web10"}

# commands
function see(){open .}
function dc(){sudo docker-compose up --build}
function dq(){sudo docker-compose down}
function profiles(){nvim ~/Documents/GitHub/shell_profiles}
function refresh(){source ~/.bash_profile}
function r(){ranger}
function ds(){sudo systemctl start docker}
#links
function g(){cd ~/Documents/GitHub}
function web10(){cd ~/Documents/GitHub/web10}
function home(){cd ~}

#start up tmux, dont let it start within itself!!!

if command -v tmux &> /dev/null && [ -n "$PS1" ] && [[ ! "$TERM" =~ screen ]] && [[ ! "$TERM" =~ tmux ]] && [ -z "$TMUX" ]; then
  exec tmux
fi
